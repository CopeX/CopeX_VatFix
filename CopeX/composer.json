{
    "name": "copex/vatfix",
    "description": "Magento2 module for accepting VAT's (UID) with country code (ex.ATU69932326) as usual in EU.",
    "type": "magento2-module",
    "version": "0.0.1",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "require": {
        "php": "~5.5.0|~5.6.0|~7.0.0",
        "magento/magento-composer-installer": "*"
    },
    "autoload": {
        "psr-4": {
            "CopeX\\VATFix\\": ""
        },
        "files": [
            "registration.php"
        ]
    },
    "extra": {
        "map": [
            [
                "*",
                "CopeX/VATFix"
            ]
        ]
    }
}